{
  "name": "wave-constant-width",
  "description": "Same wave curve with constant stroke width (simpler outline, test both refitting strategies)",
  "test_type": "fit_and_stroke",
  "input": {
    "points": [
      {"x": 50.0, "y": 100.0, "point_type": "Smooth"},
      {"x": 150.0, "y": 50.0, "point_type": "Smooth"},
      {"x": 250.0, "y": 150.0, "point_type": "Smooth"},
      {"x": 350.0, "y": 100.0, "point_type": "Smooth"}
    ],
    "is_closed": false
  },
  "stroke": {
    "widths": [5.0, 5.0, 5.0, 5.0],
    "tolerance": 0.1
  },
  "operations": ["fit_curve", "stroke", "refit_stroke", "register_skeleton", "refit_with_skeleton_correction"],
  "outputs": {
    "fitted_curve": "wave-constant-fitted",
    "stroke_outline": "wave-constant-stroke",
    "refitted_stroke": "wave-constant-refitted",
    "skeleton_corrected": "wave-constant-skeleton-correction"
  }
}
